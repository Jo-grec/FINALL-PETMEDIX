#FormInput {
    border: 1px solid #B0B0B0;
    border-radius: 20px;
    padding: 10px;
    background-color: white;
    min-height: 20px; 
    font-size: 12px;
    font-family: Lato;
}

QPushButton#create {
    min-width: 180px;         
    min-height: 50px;         
    font-size: 20px;
    font-family: Lato;
    background-color:#4A90E2;
    color: white;
    border-radius: 25px;       
    border: none;
}

QPushButton#create:hover {
    background-color: #012547;
}

QComboBox#RoleComboBox, QComboBox#RoleComboBox QLineEdit {
    padding: 10px;
    font-size: 12px;
    font-family: Lato;
    border: 1px solid  #B0B0B0;
    border-radius: 20px;
    background: white;
    color: gray;
    min-height: 20px;
}

QComboBox#RoleComboBox:hover, QComboBox#RoleComboBox QLineEdit:hover {
    border: 1px solid #007a99;
}

#RoleComboBox {
    border: 1px solid #B0B0B0;
    border-radius: 20px;
    padding: 5px 30px 10px 10px; 
    background-color: white;
    min-height: 20px;
    qproperty-iconSize: 0px;
}

/* Hide default arrow */
#RoleComboBox::down-arrow {
    image: none;
}

/* Replace drop-down with your image */
#RoleComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: right center;
    width: 15px;
    background: transparent;
    border: none;
    image: url("assets/polygon 1.png");
    padding-right: 20px;
}

QComboBox#RoleComboBox::drop-down {
    border: none;
    width: 15px;
}

#SignupPasswordContainer{
    border: 1px solid #B0B0B0;
    border-radius: 20px;
    padding: 10px;
    background-color: white;
    min-height: 20px; 
}

#PasswordFormInput{
    border: none;
    font-size: 12px;
    font-family: Lato;
    border-radius: 20px;
    min-height: 20px;
}

/* Home Page */

