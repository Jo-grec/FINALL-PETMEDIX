    font-size: 20px;
    background: transparent;
    color: #000;
}

QPushButton#navbarButton {
    background-color: transparent !important;
    color: #000;
    border: none !important;
    outline: none !important;
    padding: 12px 15px;
    text-align: left;
    font-size: 14px;
    font-weight: 500;
    min-height: 45px;
    max-height: 45px;
    margin: 2px 0px;
    display: flex;
    align-items: center;
    justify-content: flex-start;
    padding-left: 25px;
    icon-padding: 12px;
    text-indent: 0px;
    icon-position: left;
    min-width: 150px;
    text-indent: 0px;
}

QPushButton#navbarButton:checked {
    background-color: #F4F4F8 !important;
    color: #3F51B5 !important;
    border: none !important;
    outline: none !important;
}

QPushButton#navbarButton:focus {
    background-color: #F4F4F8 !important;
    color: #3F51B5 !important;
    border: none !important;
    outline: none !important;
}

QPushButton#navbarButton:pressed {
    background-color: #F4F4F8 !important;
    color: #3F51B5 !important;
    border: none !important;
    outline: none !important;
}

QPushButton#navbarButton:hover {
    background-color: #F4F4F8 !important;
    border: none !important;
    outline: none !important;
}

