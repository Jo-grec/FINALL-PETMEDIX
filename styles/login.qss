}

#signupButton {
    color: #3498db;
    font-size: 12px;
    font-family: Lato;
    border: none;
    background: transparent;
    margin-left: -10px;
}

#signupButton:hover {
    color: #2980b9;
    text-decoration: underline;
}


/* Sign up */

#signupHeader{
