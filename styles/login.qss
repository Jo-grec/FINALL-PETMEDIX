QMainWindow {
    background-color: #ffffff; 
    font-family: Lato;
}

QLineEdit {
    padding: 10px;
    font-size: 25px;
    font-family: Lato;
    border: none;
    line-height: 60;
    padding-left: 20px;
    background-color: transparent;
    border-radius: 25px;
}

 QWidget#Container {
        border: 2px solid #B0B0B0;
        border-radius: 25px;
        padding: 5px;
        background-color: white;
        line-height: 90; 
    }

QWidget#Container:hover {
    border: 1px solid #007a99;
    border-radius: 25px;
    padding: 5px;
    background-color: white;
    line-height: 90;
}

QLabel#LogoLabel {
    background: transparent; 
}

QPushButton#loginButton {
    min-width: 150px;         
    min-height: 60px;         
    font-size: 20px;
    font-family: Lato;
    background-color: #4A90E2;
    color: white;
    border-radius: 25px;       
    border: none;
    outline: none;
}

QPushButton#loginButton:hover {
    background-color: #012547;
    outline: none;
}

QPushButton#loginButton:pressed {
    outline: none;
    border: none;
}

QPushButton#loginButton:focus {
    outline: none;
    border: none;
}

QLabel#WelcomeLabel {
    color: #4A90E2;
    font-family: "League Gothic";
    font-size: 130px;
    font-style: normal;
    font-weight: 400;
    line-height: 50;
    margin-bottom: -10px;
    padding-bottom: -10px;
}

QLabel#SubwelcomeLabel {
    color: #000;
    font-family: Lato;
    font-size: 20px;
    font-style: italic;
    line-height: 50;
    font-weight: 300;
}

#forgotPasswordButton {
    color: #3498db;         
    font-size: 12px;   
    font-family: Lato;    
    border: none;            
    background: transparent;
    text-align: right;
    margin-top: -15px;
}

#forgotPasswordButton:hover {
    color: #2980b9;          
